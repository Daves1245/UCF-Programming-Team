#include <bits/stdc++.h>
using namespace std;
int main() {
    int t; cin >> t;
    while (t--) {
        string a, b; cin >> a >> b;
        int i = 0;
        while (a.size() <= b) {
            if (a.find(b) != string::npos) break;
            a += a;
            i++;
        }
    }
    return 0;
}
