#include <bits/stdc++.h>
using namespace std;
int main() {
    int t; cin >> t;
    while (t--) {
        int n; cin >> n;
        vector<long> v(n);
        if (n & 1) {
            for (long i = 0; i < n; i++) {
                v[i] = i + (n - 1) / 2;
            }
        } else {

        }
    }

    return 0;
}
